{
    /*https://github.com/palantir/tslint/blob/master/src/configs/recommended.ts*/
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rulesDirectory": [],
    "rules": {
        "quotemark": [
            "single"
        ],
        "max-classes-per-file": [
            false
        ],
        "no-string-literal": false,
        "arrow-parens": false,
        "variable-name": [
            "allow-leading-underscore"
        ],
        "no-var-requires": false
    }
}