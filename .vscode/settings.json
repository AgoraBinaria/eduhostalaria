// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada y de usuario.
{
  "tslint.autoFixOnSave": true,
  "eslint.workingDirectories": [
    "./server"
  ],
  "eslint.options": {
    "configFile": "./.eslintrc.json"
  },
  "workbench.colorCustomizations": {
    "editorError.foreground": "#ff9900",
    "editorWarning.foreground": "#BBCBC2",
    "editorInfo.foreground": "#9ea39e",
    "titleBar.activeBackground": "#011299",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#011299",
    "titleBar.inactiveForeground": "#e7e7e7"
  }
}